<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<system.diagnostics>
		<trace autoflush="true" indentsize="2">
			<listeners>
				<clear/>
				<add
					type="odm.utils.ObservableTraceListener, odm-utils"
					name="ui-logger">
					<filter 
						type="System.Diagnostics.EventTypeFilter"
						initializeData="Error"/>
				</add>
			</listeners>
		</trace>		
	</system.diagnostics>
</configuration>