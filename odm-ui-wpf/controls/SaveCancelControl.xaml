<UserControl x:Class="odm.ui.controls.SaveCancelControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
			 x:Name="ctrl"
             mc:Ignorable="d" >
    <StackPanel Orientation="Horizontal">
		<Button Name="btnSave" Content="Save" HorizontalAlignment="Left" Style="{StaticResource ButtonLeft}" />
		<Button Name="btnCancel" Content="Cancel" HorizontalAlignment="Left" Style="{StaticResource ButtonRight}" />
	</StackPanel>
</UserControl>
