<UserControl x:Class="odm.ui.controls.EditTimeBox"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="30" d:DesignWidth="200" xmlns:my="clr-namespace:odm.ui.controls">
    <Grid>
        <my:EditLable HorizontalAlignment="Left" x:Name="lText" VerticalAlignment="Top" Height="25" Width="64" Margin="0,5,0,0" />
        <my:TimePicker Margin="64,0,0,0" x:Name="eTime" VerticalAlignment="Top" Height="30" />
    </Grid>
</UserControl>
