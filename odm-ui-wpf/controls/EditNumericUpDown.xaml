<UserControl x:Class="odm.controls.EditNumericUpDown"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="24" d:DesignWidth="94">
    <Grid>
        <TextBlock Height="23" Name="eNum" Text="TextBlock" VerticalAlignment="Top" Margin="0,0,31,0" />
        <Button Content="Button" Height="12" Name="button" VerticalAlignment="Top" Style="{StaticResource UpButtonStyle}" HorizontalAlignment="Right" Width="25" />
        <Button Content="Button" Height="12" HorizontalAlignment="Right" Margin="0,12,0,0" Name="button1" Style="{StaticResource DownButtonStyle}" VerticalAlignment="Top" Width="25" />
    </Grid>
</UserControl>
